@{ Layout = null; }
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Razor</title>
    <link asp-href-include="/lib/twitter-bootstrap/css/*.min.css" rel="stylesheet" />

    <script id="jsonData" type="application/json">
        @Json.Serialize(new string[] { "JSON-data-Apple", "JSON-data-Orange", "JSON-data-Pear" })
    </script>
    <script asp-src-include="/lib/jquery/*.min.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            var list = $("#jsonDataList")
            JSON.parse($("#jsonData").text()).forEach(function (val) {
                list.append($("<li>").text(val));
            });
        });
    </script>
</head>
<body class="m-1 p-1">
    This is the List View
    @await Html.PartialAsync("MyPartial")
    @await Html.PartialAsync("MyStronglyTypedPartial", new string[] { "Apple", "Orange", "Pear" })
    
    <ul id="jsonDataList"></ul>
</body>
</html>